  //- Javascript
  script(src='/js/#{title}.min.js')
  script(src='/socket.io/socket.io.js')
  script.
    var socket = io.connect();
    // Send inital connection
    socket.on('connect', function () {
      socket.send(window.location.href);
    });
    // send page views
    window.onhashchange = function () {
      socket.send(window.location.href);
    }
